        description: ""
        action: [gotox, incr, calc, calc]
        param: ["Math.round(Math.random() * ({n} - 1) + 1)","validrounds","details+='❌{题目}答案是`{正确答案}`\\n\\n'","lastone='上一题纠错: 应该是`{正确答案}`\\n\\n'"]
